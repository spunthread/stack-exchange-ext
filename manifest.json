{
  "name": "stack-exchange-ext",
  "description": "Removes Anoying Stuff From Websites I frequently Visit.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["stackexchange.js"]
    },
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["chatgpt.js"]
    }
  ],
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  }
}
